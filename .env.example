# Discord Configuration
SOURCE_TOKEN=your_source_bot_token_here
TARGET_TOKEN=your_target_bot_token_here
# Source Guild ID (the server to clone from)
SOURCE_GUILD_ID=123456789012345678

# Target Guild ID (the server to clone to)
TARGET_GUILD_ID=987654321098765432

# Proxy Configuration (Optional)
PROXY_HOST=localhost
PROXY_PORT=8080
PROXY_USERNAME=
PROXY_PASSWORD=
PROXY_PROTOCOL=http

# Cloner Settings
CLONE_GUILDS=true
CLONE_CHANNELS=true
CLONE_ROLES=true
CLONE_EMOJIS=true

# Rate Limiting
RATE_LIMIT_DELAY=1000
MAX_CONCURRENT_REQUESTS=5
